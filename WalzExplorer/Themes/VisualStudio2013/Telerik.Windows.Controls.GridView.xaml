        <Grid>
            <Border x:Name="PART_MergedCellBorder"
                    BorderBrush="{TemplateBinding BorderBrush}"
                    BorderThickness="{TemplateBinding BorderThickness}"
                    Margin="0,0,1,1"
                    Background="{Binding Background, RelativeSource={RelativeSource TemplatedParent}}"/>
            <Border x:Name="Background_Over"
                    Grid.Column="2"
                    Grid.ColumnSpan="2"
                    Background="{telerik:VisualStudio2013Resource ResourceKey=MouseOverBrush}"
                    CornerRadius="0"
                    Margin="0,0,1,1"
                    Visibility="Collapsed"/>
            <Border x:Name="Background_Selected"
                    Grid.Column="2"
                    Grid.ColumnSpan="2"
                    Background="{telerik:VisualStudio2013Resource ResourceKey=AccentMainBrush}"
                    CornerRadius="0"
                    Margin="0,0,1,1"
                    Visibility="Collapsed"/>
            <Border x:Name="Background_Edited"
                    Grid.Column="2"
                    Grid.ColumnSpan="2"
                    BorderThickness="1"
                    IsHitTestVisible="False"
                    Margin="0,0,1,1"
                    BorderBrush="{telerik:VisualStudio2013Resource ResourceKey=AccentMainBrush}"
                    Visibility="Collapsed"/>
            <Border x:Name="Background_Current"
                    Grid.Column="2"
                    Grid.ColumnSpan="2"
                    Margin="0,0,1,1"
                    BorderThickness="1"
                    CornerRadius="0"
                    BorderBrush="{telerik:VisualStudio2013Resource ResourceKey=MarkerBrush}"
                    Visibility="Collapsed"/>
            <ContentControl x:Name="PART_ContentPresenter"
                    IsTabStop="{TemplateBinding IsTabStop}"
                    Margin="{TemplateBinding Padding}"
                    Foreground="{TemplateBinding Foreground}"
                    Content="{TemplateBinding Content}"
                    ContentTemplate="{TemplateBinding ContentTemplate}"
                    VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                    VerticalContentAlignment="Stretch"
                    HorizontalContentAlignment="{TemplateBinding HorizontalContentAlignment}"/>
        </Grid>
        <ControlTemplate.Triggers>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition Property="IsSelected" Value="True"/>
                    <Condition Property="DisplayVisualCues" Value="True"/>
                </MultiTrigger.Conditions>
                <Setter TargetName="Background_Selected" Property="Visibility" Value="Visible"/>
                <Setter TargetName="PART_ContentPresenter" Property="Foreground" Value="{telerik:VisualStudio2013Resource ResourceKey=SelectedBrush}"/>
            </MultiTrigger>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition Property="IsMouseOver" Value="True"/>
                    <Condition Property="DisplayVisualCues" Value="True"/>
                </MultiTrigger.Conditions>
                <Setter TargetName="Background_Over" Property="Visibility" Value="Visible"/>
            </MultiTrigger>
            <Trigger Property="IsInEditMode" Value="True">
                <Setter TargetName="PART_MergedCellBorder" Property="Visibility" Value="Collapsed"/>
                <Setter TargetName="PART_ContentPresenter" Property="Visibility" Value="Collapsed"/>
                <Setter TargetName="Background_Over" Property="Visibility" Value="Collapsed"/>
                <Setter TargetName="Background_Selected" Property="Visibility" Value="Collapsed"/>
                <Setter TargetName="Background_Edited" Property="Visibility" Value="Visible"/>
                <Setter TargetName="Background_Edited" Property="Padding" Value="1"/>
            </Trigger>
            <Trigger Property="IsEnabled" Value="False">
                <Setter TargetName="PART_ContentPresenter" Property="Opacity" Value="0.4"/>
                <Setter TargetName="Background_Current" Property="Opacity" Value="0.4"/>
            </Trigger>
            <Trigger Property="IsCurrent" Value="True">
                <Setter TargetName="Background_Current" Property="Visibility" Value="Visible"/>
            </Trigger>
        </ControlTemplate.Triggers>
