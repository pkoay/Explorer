    <ControlTemplate x:Key="GridViewGroupPanelItemTemplate" TargetType="grid:GridViewGroupPanelItem">
        <StackPanel x:Name="PART_GroupPanelItemStackPanel"
                Orientation="Horizontal"
                VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                Background="{TemplateBinding Background}">
            <Grid HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}">
                <Grid.RowDefinitions>
                    <RowDefinition MinHeight="4" Height="Auto"/>
                    <RowDefinition/>
                </Grid.RowDefinitions>
                <Path x:Name="PART_InsertionPoint"
                        Data="M4,0L4,7 0,3.35z"
                        Stretch="Fill"
                        RenderTransformOrigin="0.5,0.5"
                        Fill="{telerik:VisualStudio2013Resource ResourceKey=StrongBrush}"
                        HorizontalAlignment="Center"
                        Width="4"
                        Height="6"
                        VerticalAlignment="Top"
                        Margin="3 0"
                        Visibility="{TemplateBinding InsertionPointVisibility}"
                        SnapsToDevicePixels="True">
