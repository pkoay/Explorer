                                    MaxWidth="250"
                                    FontSize="12"
                                    FontFamily="Segoe UI"
                                    VerticalAlignment="Center"
                                    Foreground="{telerik:VisualStudio2013Resource ResourceKey=SelectedBrush}"
                                    Text="{Binding}"
                                    TextWrapping="Wrap"/>
