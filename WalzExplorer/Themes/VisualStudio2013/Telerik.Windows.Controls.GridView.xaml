                        Fill="{telerik:VisualStudio2013Resource ResourceKey=AccentMainBrush}"
                        HorizontalAlignment="Center"
                        Opacity="0"
                        Stretch="Fill"
                        Width="8"
                        RenderTransformOrigin="0.5,0.5"
                        Height="8"
                        VerticalAlignment="Center"
                        SnapsToDevicePixels="True">
                    <Path.RenderTransform>
                        <TransformGroup>
                            <ScaleTransform/>
                            <SkewTransform/>
