<Window x:Class="WalzExplorer.Windows.FeedbackDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:model="clr-namespace:WalzExplorer.Windows"
       
        Title="Feedback" Height="232" Width="300" ShowInTaskbar="False" Topmost="True" ResizeMode="NoResize" Loaded="Window_Loaded">
    <Window.Resources>
        <model:FeedbackDialogViewModel x:Key="model"/>
    </Window.Resources>
    <Grid DataContext="{DynamicResource model}">
        <ComboBox Name="cmbType" HorizontalAlignment="Left" Margin="145,81,0,0" VerticalAlignment="Top" Width="120" ItemsSource="{Binding TypeList}"  DisplayMemberPath="Title"/>
        <Label Content="Feedback User:" HorizontalAlignment="Left" Margin="35,81,0,0" VerticalAlignment="Top" Width="91"/>
        <Button Name="btnCancel" Content="Cancel" HorizontalAlignment="Left" Margin="35,156,0,0" VerticalAlignment="Top" Width="75" Click="btnCancel_Click"/>
        <Button Name="btnOK" Content="OK" HorizontalAlignment="Left" Margin="190,156,0,0" VerticalAlignment="Top" Width="75" Click="btnOK_Click"/>

    </Grid>
</Window>
